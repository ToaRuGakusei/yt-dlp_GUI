<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:yt_dlp_GUI"
        xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls" x:Class="yt_dlp_GUI.Command"
        mc:Ignorable="d"
        Title="Setting" Height="334" Width="377">
    <Grid Margin="0,0,0,-6">
        <TextBox x:Name="textbox" TextWrapping="Wrap" VerticalAlignment="Top" Margin="10,69,10,0" Height="58"/>
        <Button x:Name="OK" Content="OK" HorizontalAlignment="Center" Margin="0,132,0,0" Width="60" Click="OK_Click" Height="29" VerticalAlignment="Top"/>
        <Label  Content="コマンドラインを入力してください。" HorizontalAlignment="Left" Margin="10,38,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="cmd" Content="自分でコマンドラインを指定する" Margin="10,10,0,0" Checked="cmd_Checked" Unchecked="cmd_Unchecked" Height="23" VerticalAlignment="Top" HorizontalAlignment="Left" Width="171"/>

    </Grid>
</Window>
